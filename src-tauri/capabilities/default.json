{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:menu:allow-new",
    "core:tray:allow-new",
    "core:tray:allow-set-icon",
    "core:tray:allow-set-menu",
    "core:tray:allow-set-show-menu-on-left-click",
    "dialog:default",
    "fs:default",
    "shell:allow-open"
  ]
}
